<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="imcompressor">
	<schema id="com.github.huluti.ImCompressor" path="/com/github/huluti/ImCompressor/">
	  <key type="b" name="new-file">
      <default>true</default>
      <summary>Enable automatic zoom</summary>
      <description>Whether or not save the compressed image into a new file.</description>
    </key>
	  <key type="b" name="dark-theme">
      <default>false</default>
      <summary>Enable dark theme</summary>
      <description>Use dark theme for the window.</description>
    </key>
	</schema>
</schemalist>
